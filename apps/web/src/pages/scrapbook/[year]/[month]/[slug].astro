---
import type { PagePayload, PageProps } from '../../../../../types';
import { getPosts } from '../../../../data/posts';

type Props = PageProps;

export async function getStaticPaths(): Promise<PagePayload[]> {
  return getPosts(Astro.site);
}

const { title, body, publishedDate, meta } = Astro.props;
---

<LayoutPost variant="post" title={title} meta={meta} publishedDate={publishedDate} body={body} />
