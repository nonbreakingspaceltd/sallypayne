---
import { getPages } from '../data/pages';
import LayoutPage from '../layouts/Page.astro';

export async function getStaticPaths() {
  return getPages(['contact']);
}

const { body, title, image, meta } = Astro.props;
---

<LayoutPage
  variant="page"
  title={title}
  meta={meta}
  body={body}
  image={image}
/>
