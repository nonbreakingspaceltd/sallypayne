    "classnames": "2.5.1",
    "cross-fetch": "4.1.0",
    "date-fns": "4.1.0",
    "dotenv": "17.2.3",
    "focus-lock": "1.3.6",
    "modern-normalize": "3.0.1",
    "vue": "3.5.24",
    "vue-dragscroll": "4.0.6"
