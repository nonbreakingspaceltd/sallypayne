{
  "name": "@sallypayne/web",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "dev": "yarn start",
    "build": "astro build && yarn transform",
    "serve": "yarn build && npx serve --config ../serve.json dist",
    "start": "astro dev",
    "transform": "yarn transform:css && yarn transform:404",
    "transform:404": "node src/transforms/404.js",
    "transform:css": "node src/transforms/css.js",
    "transform:html": "node src/transforms/html.js"
  },
  "devDependencies": {
    "@sallypayne/snowpack-plugin-vue-svg": "0.0.1",
    "@snowpack/plugin-postcss": "^1.4.3",
    "astro": "^0.19.1",
    "autoprefixer": "^10.3.1",
    "cssnano": "^5.0.8",
    "dotenv": "^10.0.0",
    "fs-extra": "^10.0.0",
    "glob": "^7.1.7",
    "htmlnano": "^1.0.0",
    "postcss": "^8.3.6",
    "postcss-at-rules-variables": "^0.3.0",
    "postcss-custom-media": "^8.0.0",
    "postcss-import": "^14.0.2",
    "postcss-mixins": "^8.1.0",
    "postcss-nested": "^5.0.6",
    "postcss-pxtorem": "^6.0.0",
    "purgecss": "^4.0.3"
  },
  "dependencies": {
    "modern-normalize": "^1.1.0"
  }
}
