{
  "name": "@sallypayne/web",
  "private": true,
  "version": "1.1.0",
  "scripts": {
    "astro:build": "astro build",
    "astro:dev": "astro dev",
    "astro:preview": "astro preview",
    "build": "yarn astro:build && yarn transform",
    "dev": "yarn astro:dev",
    "serve": "yarn build && npx serve --config ../serve.json dist",
    "start": "yarn dev",
    "transform": "yarn transform:css && yarn transform:html",
    "transform:css": "node -r esbuild-runner/register src/transforms/css.js",
    "transform:html": "node -r esbuild-runner/register src/transforms/html.js"
  },
  "devDependencies": {
    "@astrojs/renderer-vue": "^0.3.0",
    "@sanity/image-url": "^1.0.1",
    "@vitejs/plugin-vue": "^2.0.1",
    "astro": "^0.22.11",
    "autoprefixer": "^10.4.2",
    "cssnano": "^5.0.15",
    "dotenv": "^11.0.0",
    "esbuild-runner": "^2.2.1",
    "fs-extra": "^10.0.0",
    "glob": "^7.2.0",
    "htmlnano": "^2.0.0",
    "postcss": "^8.4.5",
    "postcss-at-rules-variables": "^0.3.0",
    "postcss-custom-media": "^8.0.0",
    "postcss-import": "^14.0.2",
    "postcss-mixins": "^9.0.1",
    "postcss-nested": "^5.0.6",
    "postcss-pxtorem": "^6.0.0",
    "purgecss": "^4.1.3",
    "vite-svg-loader": "^3.1.2"
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "cross-fetch": "^3.1.4",
    "date-fns": "^2.28.0",
    "focus-lock": "^0.10.1",
    "html-entities": "^2.3.2",
    "modern-normalize": "^1.1.0",
    "sanity-blocks-vue-component": "^1.0.1",
    "vue": "^3.2.20",
    "vue-dragscroll": "^3.0.1"
  }
}
