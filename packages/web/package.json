{
  "name": "@sallypayne/web",
  "private": true,
  "version": "1.1.0",
  "scripts": {
    "astro:build": "yarn products && astro build",
    "astro:dev": "astro dev",
    "astro:preview": "astro preview",
    "products": "node -r esbuild-runner/register -r dotenv/config scripts/productsJson.js",
    "build": "yarn astro:build",
    "dev": "yarn astro:dev",
    "serve": "yarn build && npx serve --config ../serve.json dist",
    "start": "yarn dev",
    "transform": "yarn transform:css && yarn transform:html",
    "transform:css": "node -r esbuild-runner/register scripts/optimizeCss.js",
    "transform:html": "node -r esbuild-runner/register scripts/optimizeHtml.js"
  },
  "devDependencies": {
    "@astrojs/renderer-vue": "0.3",
    "@sanity/image-url": "^1.0.1",
    "astro": "0.22",
    "autoprefixer": "^10.4.7",
    "cssnano": "^5.1.11",
    "dotenv": "^16.0.1",
    "esbuild-runner": "^2.2.1",
    "fs-extra": "^10.1.0",
    "glob": "^8.0.3",
    "htmlnano": "^2.0.2",
    "postcss": "^8.4.14",
    "postcss-at-rules-variables": "^0.3.0",
    "postcss-custom-media": "^8.0.2",
    "postcss-import": "^14.1.0",
    "postcss-mixins": "^9.0.2",
    "postcss-nested": "^5.0.6",
    "postcss-pxtorem": "^6.0.0",
    "postcss-sorting": "^7.0.1",
    "purgecss": "^4.1.3",
    "vite-svg-loader": "^3.3.0"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "axios-cache-interceptor": "^0.10.4",
    "classnames": "^2.3.1",
    "cross-fetch": "^3.1.5",
    "date-fns": "^2.28.0",
    "focus-lock": "^0.11.2",
    "html-entities": "^2.3.3",
    "modern-normalize": "^1.1.0",
    "sanity-blocks-vue-component": "^1.0.1",
    "vite-plugin-environment": "^1.1.1",
    "vue": "^3.2.37",
    "vue-dragscroll": "^3.0.1"
  }
}
