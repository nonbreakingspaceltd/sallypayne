---
import LayoutBase from '../layouts/Base.astro';
import LayoutHome from '../layouts/Home.astro';

const { title = 'Sally Payne - Illustration & Surface Pattern Design' } = Astro.props;

const meta = {
  description: `Welcome to Sally Payne's world of illustration and surface pattern design.`
};
---
<LayoutBase modifier="home" title={title} meta={meta}>
  <LayoutHome title={title} />
</LayoutBase>
