---
import LayoutPage from '../layouts/Page.astro';
import ContactForm from '../components/ContactForm/ContactForm.vue';
import { getPage } from '../data/pages';

const response = await getPage('contact');

const { body, title, image, meta } = response.props;
---
<LayoutPage variant="contact" title={title} meta={meta} body={body} image={image} >
  <ContactForm client:idle />
</LayoutPage>

